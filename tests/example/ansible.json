{
    "inventory.base.All": {
        "vars": {
            "minify": false,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": false,
            "version": "develop"
        },
        "children": [
            "inventory.env.Staging",
            "inventory.project.BackEnd",
            "inventory.project.FrontEnd",
            "inventory.env.Production"
        ]
    },
    "inventory.env.Staging": {
        "vars": {
            "minify": false,
            "redis_host": "localhost",
            "run_tests": true,
            "use_redis": false,
            "version": "develop"
        },
        "children": [
            "inventory.stages.DevelopHost",
            "inventory.stages.StagingHost"
        ]
    },
    "inventory.project.BackEnd": {
        "vars": {
            "minify": false,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": true,
            "version": "develop"
        },
        "children": [
            "inventory.stages.DevelopHost",
            "inventory.prod.ProdBackEnd",
            "inventory.stages.StagingHost"
        ]
    },
    "inventory.project.FrontEnd": {
        "vars": {
            "minify": false,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": false,
            "version": "develop"
        },
        "children": [
            "inventory.stages.DevelopHost",
            "inventory.prod.ProdFrontEnd",
            "inventory.stages.StagingHost"
        ]
    },
    "inventory.stages.DevelopHost": {
        "vars": {
            "ansible_host": "develop_hostname",
            "extra": {
                "debug": 1
            },
            "minify": false,
            "redis_host": "localhost",
            "run_tests": true,
            "use_redis": true,
            "version": "develop"
        },
        "hosts": [
            "develop",
            "develop_sidebranch"
        ]
    },
    "inventory.env.Production": {
        "vars": {
            "minify": true,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": true,
            "version": "master"
        },
        "children": [
            "inventory.prod.ProdBackEnd",
            "inventory.prod.ProdFrontEnd"
        ]
    },
    "inventory.prod.ProdBackEnd": {
        "vars": {
            "minify": true,
            "redis_host": "prod_redis_hostname",
            "run_tests": false,
            "use_redis": true,
            "version": "master"
        },
        "hosts": [
            "prod_backend1",
            "prod_backend2"
        ]
    },
    "inventory.prod.ProdFrontEnd": {
        "vars": {
            "minify": true,
            "redis_host": "localhost",
            "run_tests": false,
            "use_redis": true,
            "version": "master"
        },
        "hosts": [
            "prod_frontend1",
            "prod_frontend2"
        ]
    },
    "inventory.stages.StagingHost": {
        "vars": {
            "ansible_host": "master_hostname",
            "extra_branches": [
                "foo",
                "bar"
            ],
            "minify": false,
            "redis_host": "localhost",
            "run_tests": true,
            "use_redis": true,
            "version": "master"
        },
        "hosts": [
            "staging"
        ]
    },
    "_meta": {
        "hostvars": {
            "develop": {
                "ansible_host": "develop_hostname",
                "extra": {
                    "debug": 1
                },
                "minify": false,
                "redis_host": "localhost",
                "run_tests": true,
                "use_redis": true,
                "version": "develop"
            },
            "develop_sidebranch": {
                "ansible_host": "sidebranch_hostname",
                "extra": {
                    "debug": 1
                },
                "minify": false,
                "redis_host": "localhost",
                "run_tests": true,
                "use_redis": true,
                "version": "sidebranch_name"
            },
            "prod_backend1": {
                "ansible_hostname": "app001.prod.dom",
                "minify": true,
                "num": 1,
                "redis_host": "prod_redis_hostname",
                "run_tests": false,
                "use_redis": true,
                "version": "master"
            },
            "prod_backend2": {
                "ansible_hostname": "app002.prod.dom",
                "minify": true,
                "num": 2,
                "redis_host": "prod_redis_hostname",
                "run_tests": false,
                "use_redis": true,
                "version": "master"
            },
            "prod_frontend1": {
                "ansible_hostname": "www001.prod.dom",
                "minify": true,
                "num": 1,
                "redis_host": "localhost",
                "run_tests": false,
                "use_redis": true,
                "version": "master"
            },
            "prod_frontend2": {
                "ansible_hostname": "www002.prod.dom",
                "minify": true,
                "num": 2,
                "redis_host": "localhost",
                "run_tests": false,
                "use_redis": true,
                "version": "master"
            },
            "staging": {
                "ansible_host": "master_hostname",
                "extra_branches": [
                    "foo",
                    "bar"
                ],
                "minify": false,
                "redis_host": "localhost",
                "run_tests": true,
                "use_redis": true,
                "version": "master"
            }
        }
    }
}